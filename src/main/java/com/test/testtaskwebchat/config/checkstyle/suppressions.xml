<?xml version="1.0"?>
<!DOCTYPE suppressions PUBLIC
        "-//Checkstyle//DTD SuppressionFilter Configuration 1.2//EN"
        "https://checkstyle.org/dtds/suppressions_1_2.dtd">

<suppressions>
    <suppress checks=".*" files=".*[\\/]test[\\/].*"/>

    <suppress checks=".*" files=".*[\\/]generated[\\/].*"/>

    <suppress checks="JavadocMethod" files=".*[\\/]model[\\/].*"/>
    <suppress checks="JavadocType" files=".*[\\/]model[\\/].*"/>

    <suppress checks="MethodLength" files=".*[\\/]config[\\/].*"/>

    <suppress checks="FinalParameters" files=".*"/>
    <suppress checks="JavadocMethod" files=".*" lines="1-1000"/>
</suppressions>